    "react-datepicker": "0.29.0",
    "react-dom": "15.2.0",
    "react-intl": "2.1.3",
    "react-maskedinput": "3.2.0",
    "react-redux": "4.4.5",
    "react-router": "2.5.2",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0"
  },
